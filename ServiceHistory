Create PROCEDURE [dbo].[ServiceHistory]
(
@Message varchar(8000),
@RefId int
)
AS
BEGIN
SET NOCOUNT ON 
-- SET NOCOUNT ON added to prevent extra result sets from
-- interfering with SELECT statements.
BEGIN TRY
BEGIN TRAN
DECLARE @ErrorMessage VARCHAR(8000);
DECLARE @ErrorSeverity INT;
DECLARE @ErrorState INT;


DECLARE @CreatedDate DATETIME 

SET @CreatedDate=GETDATE()

--Get Visitor Id if it exists already in VisitorMaster with Email ID

INSERT INTO dbo.ExternalServiceHistory(TrackMessage,ExternalRefId,LastUpdatedDate)
VALUES(@Message,@RefId,@CreatedDate)
		

COMMIT TRAN
END TRY

BEGIN CATCH
ROLLBACK TRAN
IF (@@ERROR <> 0)
BEGIN	
SELECT
@ErrorMessage =ERROR_MESSAGE(),
@ErrorSeverity = ERROR_SEVERITY(),
@ErrorState = ERROR_STATE();
RAISERROR (@ErrorMessage, @ErrorSeverity, @ErrorState)
END
END CATCH	

RETURN 1 
SET NOCOUNT OFF;	
END

