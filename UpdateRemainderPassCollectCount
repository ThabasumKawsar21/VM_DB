CREATE PROCEDURE [dbo].[UpdateRemainderPassCollectCount]
	@remaindercount int,
	@ParentreferenceID int
AS
BEGIN
	-- SET NOCOUNT ON added to prevent extra result sets from
	-- interfering with SELECT statements.
	SET NOCOUNT ON;
	Update clientrequestdetails 
SET RemainderPassCollect=@remaindercount 
where ParentRefernceID=@ParentreferenceID

END
GO
