CREATE TABLE [dbo].[VisitDetails_Backup](
[ID] [int] IDENTITY(1,1),
	[VisitDetailsID] [int] NOT NULL,
	[RequestID] [int] NOT NULL,
	[Date] [date] NULL,
	[ActualOutTime] [time](7) NULL,
	[BadgeNo] [varchar](50) NULL,
	[BadgeIssuedDate] [datetime] NULL,
	[BadgeStatus] [varchar](100) NULL,
	[IssuedBy] [varchar](50) NULL,
	[LastUpdatedDate] [datetime] NULL,
