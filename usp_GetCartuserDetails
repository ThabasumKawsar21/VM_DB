CREATE PROCEDURE [dbo].[usp_GetCartuserDetails](@CartID int,
@AssociateID varchar(10))
       -- Add the parameters for the stored procedure here
       
AS
BEGIN
       -- SET NOCOUNT ON added to prevent extra result sets from
       -- interfering with SELECT statements.
       SET NOCOUNT ON;

    -- Insert statements for procedure here
       Declare @Cart int=(Select EuipmqntUniqID  from tbl_VisitorCart where CartID=@CartID)
   Declare @Count int=(Select Count(1) as Count from tbl_DummyEquipmentDetails where CartID=@Cart)
       Select CartID,VisitorType,MobileNo,Organization,VisitorName,NativeCountry,LocationName,LocationID,HostID,Date,FromTime,ToTime,isEquipment,IsVisitorConfidance,isDatacenterAccess,LocationCount,Others,@Count as Count,EuipmqntUniqID from tbl_VisitorCart
       where CartID=@CartID and AssociateID=@AssociateID
END
